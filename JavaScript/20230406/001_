//주소값 저장

let arr1 = [1,2,3];
let arr2 = arr1;
console.log(arr2);
//(3)[1, 2, 3]

arr1[0] = 10;
console.log(arr2);
//(3)[10, 2, 3]

arr1 = [10,20];
console.log(arr2);
//arr1 = (2) [10, 20]
//arr2 = (3) [1, 2, 3]

// 안의 해당 인덱스 값을 바꾸는 것은 그대로적용하는데(주소저장이라)


// 비교해보세요.
let value1 = 10
let value2 = value1
console.log(value2)
//10

value1 = 20
console.log(value2)
//10

